@model JobSeekersByCityViewModel
@{
    // Builds messages and titles that are added to the DataTables.net plugin
    // see: https://datatables.net/extensions/buttons/examples/html5/titleMessage.html

    // Report Type parameter
    string cityReportTypeHeader = Model.GetCityReportTypeHeader(SelectLists);

    // Date Registered parameter
    string exportMessage = Model.GetDateRegisteredHeader();

    // Parameters header
    string paramsHeader = exportMessage + HeaderExtensions.GetReportDateHeader();
}

@Html.Raw(paramsHeader)

<script>
    window.datatablesExportMessageExcel = '@Html.Raw(exportMessage.ToExcelParams())';
    window.datatablesExportTitleExcel = '@(ViewBag.ReportName + " - " + cityReportTypeHeader): @DateTime.Now';
</script>