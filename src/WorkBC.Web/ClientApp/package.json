  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "startJbSearch": "ng serve jb-search",
    "startJbAccount": "ng serve jb-account",
    "build": "ng build",
    "buildJbSearch": "ng build jb-search --configuration production --output-hashing=none",
    "buildJbAccount": "ng build jb-account --configuration production --output-hashing=none",
    "buildJbLib": "ng build jb-lib",
    "watchJbSearch": "ng build jb-search --watch",
    "watchJbAccount": "ng build jb-account --watch",
    "watchJbLib": "ng build jb-lib --watch",
    "watchAll": "concurrently \"npm:watchJbAccount\" \"npm:watchJbSearch\" \"npm:watchJbLib\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
